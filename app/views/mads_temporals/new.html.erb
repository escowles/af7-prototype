<h1>Create a new MADS Temporal</h1>
<%= form_for @mads_temporal do |f| %>
  <%= label_tag 'mads_temporal[name]', "Name" %>
  <%= text_field_tag 'mads_temporal[name]' %>
  <br>

  <%= label_tag 'mads_temporal[elementList_attributes][][temporalElement_attributes][][elementValue]', "Temporal Element" %>
  <%= text_field_tag 'mads_temporal[elementList_attributes][][temporalElement_attributes][][elementValue]' %>
  <br>

  <%= label_tag 'mads_temporal[externalAuthority]', "Authority" %>
  <%= text_field_tag 'mads_temporal[externalAuthority]' %>
  <br>

  <%= label_tag 'mads_temporal[scheme]', "Scheme" %>
  <%= select_tag 'mads_temporal[scheme]', options_for_select(@mads_schemes), {:include_blank => 'Select Scheme'} %>
  <br>

  <%=f.submit "Create", class: "btn btn-primary" %>
<%end%>
